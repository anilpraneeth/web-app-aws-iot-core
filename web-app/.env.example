# AWS IoT Core Configuration
REACT_APP_AWS_REGION=your-region
REACT_APP_AWS_IOT_ENDPOINT=your-iot-endpoint.iot.region.amazonaws.com
REACT_APP_CERT_PATH=certificates/certificate.pem.crt
REACT_APP_KEY_PATH=certificates/private.pem.key

# AWS Credentials (Optional - if not using IAM roles)
REACT_APP_AWS_ACCESS_KEY_ID=your-access-key-id
REACT_APP_AWS_SECRET_ACCESS_KEY=your-secret-access-key
# Optional: REACT_APP_AWS_SESSION_TOKEN=your-session-token 